{
  "name": "shared-tools",
  "version": "0.0.1",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha src/**/*.test.js --watch --require babel-core/register --require babel-polyfill",
    "eslint": "eslint --ignore-path .gitignore ./",
    "clean": "rimraf lib/",
    "compile": "babel src/ --ignore **/*.test.js --out-dir lib/ --copy-files --source-maps",
    "build": "npm-run-all clean compile"
  },
  "keywords": [],
  "author": "Vadim Sartakov",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  }
}
